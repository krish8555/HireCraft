# Admin Credentials
ADMIN_USERNAME=your_admin_username_here
ADMIN_PASSWORD=your_secure_password_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Connect to Supabase via connection pooling (for Prisma)
DATABASE_URL="postgresql://postgres.your-project:your-password@aws-0-region.pooler.supabase.com:6543/postgres?pgbouncer=true"

# Direct connection to the database (Used for migrations)
DIRECT_URL="postgresql://postgres.your-project:your-password@aws-0-region.pooler.supabase.com:5432/postgres"

# Gemini AI API Keys (dedicated for specific tasks)
GEMINI_API_KEY_RESUME_PARSING=your_gemini_api_key_for_resume_parsing
GEMINI_API_KEY_JD_MATCHING=your_gemini_api_key_for_jd_matching
GEMINI_API_KEY_INTERVIEW=your_gemini_api_key_for_interview
